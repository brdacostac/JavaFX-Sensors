<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="view.FXMLWindow"
            prefHeight="400.0" prefWidth="600.0">

    <HBox>
        <VBox>
            <TreeView fx:id="captorTreeView"/>
            <Button fx:id="addCaptorButton" text="Add Captor" onAction="#addCaptor"/>
            <Button fx:id="afficher" text="Afficher" onAction="#afficher"/>
        </VBox>
        <TextField fx:id="name"></TextField>
        <Label fx:id="temperature" text="salut"/>
    </HBox>


</AnchorPane>
