<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.collections.FXCollections?>
<?import javafx.scene.text.Text?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="view.FXMLWindow"
            prefHeight="500.0" prefWidth="700.0">

    <HBox spacing="10">
        <VBox spacing="10">
            <TreeView fx:id="captorTreeView"/>
            <HBox spacing="10">
            <Button fx:id="addCaptorButton" text="Add Captor" onAction="#addCaptor"/>
                <Button fx:id="deleteCaptorButton" text="Delete Captor" onAction="#deleteCaptor"/>
            </HBox>
        </VBox>
        <TextField fx:id="name"/>
        <Label fx:id="id"/>
        <Button fx:id="imageTemperature" text="Temperature" onAction="#windowImage"/>
    </HBox>

</AnchorPane>
